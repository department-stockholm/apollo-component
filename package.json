{
  "name": "@department/apollo-component",
  "version": "1.10.0-canary.0",
  "description": "Render component for your Apollo Client",
  "repository": "department-stockholm/apollo-component",
  "homepage": "https://github.com/department-stockholm/apollo-component",
  "bugs": "https://github.com/department-stockholm/apollo-component/issues",
  "main": "dist/index.js",
  "module": "dist/index.esm.mjs",
  "browser": "dist/index.umd.js",
  "umd:main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "watch": "microbundle watch --strict --no-compress",
    "build": "npm run build:main && npm run build:mock && npm run build:state",
    "build:main": "microbundle build --external all --strict src/index.ts",
    "build:mock": "microbundle build --external all --strict src/mock/*.ts -o mock.js -f cjs && rm -rf mock state *.d.ts",
    "build:state": "microbundle build --external all --strict src/state/*.ts -o state.js -f cjs && rm -rf mock state **.d.ts",
    "clean": "rm -rf dist/* state.js* mock.js* mock state *.d.ts",
    "prepare": "npm run build",
    "release": "release",
    "postpublish": "npm run clean"
  },
  "author": "Robert Sk√∂ld (https://department.se)",
  "license": "ISC",
